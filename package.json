{
  "name": "img",
  "version": "0.0.10",
  "description": "",
  "main": "app.js",
  "scripts": {
    "test": "echo '  << NO TESTS DEFINED >>' && echo",
    "update": "npm up && cp -R node_modules/leaflet/dist src/frontend/leaflet/",
    "build": "npm run webpack && echo '(build 50% done)' && npm run tsc -- --p tsconfig.backend.json && echo '(build 100% done)'",
    "build:prod": "NODE_ENV=production npm run build && npm run uglify:frontend",
    "antwerp": "echo antwerp > brand.txt",
    "uglify:frontend": "uglifyjs -c -m -o dist/htdocs/bundle.js dist/htdocs/bundle.js",
    "start": "./src/startup.sh",
    "starts": "if [ \"$1\" = \"dev\" ]; then npm run start:dev; else node 'src/startup.js'; fi #",
    "start:dev": "NODE_ENV=development HTTP_PORT=8080 npm-run-all --parallel watch:frontend watch:backend nodemon:server",
    "watch:frontend": "webpack --watch",
    "watch:backend": "tsc --watch --p tsconfig.backend.json",
    "nodemon:server": "nodemon dist/app.js",
    "start:prod": "NODE_ENV=production HTTP_PORT=8072 node dist/app.js",
    "forever:stop": "forever stop dist/app.js",
    "forever:start": "NODE_ENV=production HTTP_PORT=8072 forever start dist/app.js",
    "forever": "NODE_ENV=production HTTP_PORT=8072 forever restart dist/app.js",
    "webpack": "webpack",
    "tsc": "tsc",
    "typings": "typings",
    "refresh": "git pull && npm i && npm run build:prod && npm run forever",
    "push:master": "git checkout master && git merge develop && git push && git checkout develop"
  },
  "author": "tesse@informatik.uni-hamburg.de",
