{
	"compilerOptions": {
		"target": "es6",
		"module": "commonjs",
		"outDir": "dist",
		"lib": [
			"es7"
		],
		"types": [
			"node"
		],
		"typeRoots": [
			"node_modules/@types",
			"node_modules/technicalindicators/declarations"
		],
		"sourceMap": true
	},
	"compileOnSave": true,
	"files": [
		"src/backend/app.ts"
	],
	"include": [
		"src/backend/**/*.ts"
	],
	"exclude": [
		"node_modules",
		"src/fontend"
	]
}